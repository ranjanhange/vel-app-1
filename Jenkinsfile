pipeline {
		agent {
					label {
								label 'built-in'
								customWorkspace '/mnt/myproject'
							
					}
		}
		
		stages {
		
				stage ('deploy'){
							steps {		
									echo "hello this is master branch"
						}							
				
				}
				
				stage ('start'){
							steps {
									echo "this is master branch"
						}							
				
				}
		}
}
